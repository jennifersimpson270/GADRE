jQuery(document).on("mouseover", "#wpls-logo-showcase-slider-1 .slick-slide", function (){
let thisImage=jQuery(this).find("img");
jQuery.each(objImgJSON, function (key, val){
if(thisImage.attr("alt").toLowerCase()==val.altText.toLowerCase()){
thisImage.attr("src", val.imgHover);
thisImage.attr("alt", val.altTextHover);
}});
});
jQuery(document).on("mouseout", "#wpls-logo-showcase-slider-1 .slick-slide", function (){
let thisImage=jQuery(this).find("img");
jQuery.each(objImgJSON, function (key, val){
if(thisImage.attr("alt").toLowerCase()==val.altTextHover.toLowerCase()){
thisImage.attr("src", val.imgNormal);
thisImage.attr("alt", val.altText);
}});
});
