<script>(function jqIsReady_552(){if(typeof jQuery==="undefined"){jqIsReady_552();}else{
jQuery(document).ready(function (){
var isAndroid=/ Android/i.test(navigator.userAgent.toLowerCase());
var gadreInterval=setInterval(gadreStoreLocatorFunc, 10);
var gadreIntervalRow;
function gadreStoreLocatorFunc(){
if(jQuery(".footable_parent").find(".ninja_footable tbody").length){
jQuery(".footable_parent").find(".ninja_footable tbody").hide();
jQuery(".footable_parent").find(".footable-header").hide();
jQuery(".footable_parent").find(".footable-pagination-wrapper").hide();
gadreStoreLocatorFuncStop();
}}
function gadreStoreLocatorFuncStop(){
clearInterval(gadreInterval);
}
function gadreStoreLocatorFuncRowIssue(){
clearInterval(gadreIntervalRow);
}
function checkValueAndShowTable(sValue){
if(sValue){
jQuery(".footable_parent").find(".ninja_footable tbody").show();
jQuery(".footable_parent").find(".ninja_footable").find(".footable-details tbody").show();
jQuery(".footable_parent").find(".footable-header").show();
jQuery(".footable_parent").find(".footable-pagination-wrapper").show();
gadreIntervalRow=setInterval(
function(){
if(!jQuery(".footable_parent").find(".ninja_footable").find(".footable-details tbody").is(":visible")){
jQuery(".footable_parent").find(".ninja_footable").find(".footable-details tbody").show();
gadreStoreLocatorFuncRowIssue();
}}, 10);
}else{
jQuery(".footable_parent").find(".ninja_footable tbody").hide();
jQuery(".footable_parent").find(".ninja_footable").find(".footable-details tbody").hide();
jQuery(".footable_parent").find(".footable-header").hide();
gadreInterval=setInterval(
function(){
if(jQuery(".footable_parent").find(".footable-pagination-wrapper").length){
jQuery(".footable_parent").find(".footable-pagination-wrapper").hide();
gadreStoreLocatorFuncStop();
}}, 10);
}}
jQuery(document).on("keyup touchend", ".footable-filtering-search input", function (e){
if(!isAndroid){
checkValueAndShowTable(jQuery(this).val());
}else{
if(e.keyCode==13){
checkValueAndShowTable(jQuery(this).val());
}
else if(e.keyCode==8){
checkValueAndShowTable(jQuery(this).val());
}}
});
jQuery(document).on("click", ".footable-filtering-search .input-group-btn", function (){
if(jQuery(this).find(".fooicon-search").length){
jQuery(".footable_parent").find(".ninja_footable tbody").hide();
jQuery(".footable_parent").find(".footable-header").hide();
jQuery(".footable_parent").find(".footable-pagination-wrapper").hide();
}
checkValueAndShowTable(jQuery(".footable-filtering-search input").val());
});
});}})();</script>
